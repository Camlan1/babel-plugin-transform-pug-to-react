{
  "name": "babel-plugin-transform-pug-to-react",
  "version": "0.1.1",
  "description": "Babel plugin for pug to react",
  "main": "build/index.js",
  "config": {
    "DEST": "build"
  },
  "scripts": {
    "build": "babel src -d $npm_package_config_DEST",
    "prepublish": "npm run build",
    "test": "ava --tap"
  },
  "author": "jigsaw (http://jgs.me)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "dependencies": {
    "babel-core": "^6.8.0",
    "babel-preset-react": "^6.5.0",
    "pug": "^2.0.0-alpha6"
  },
  "ava": {
    "require": "babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-jigsaw/babel-plugin-transform-pug-to-react.git"
  },
  "bugs": {
    "url": "https://github.com/e-jigsaw/babel-plugin-transform-pug-to-react/issues"
  },
  "homepage": "https://github.com/e-jigsaw/babel-plugin-transform-pug-to-react#readme",
  "keywords": [
    "babel",
    "babel-plugin",
    "react",
    "pug",
    "jade",
    "template",
    "html"
  ]
}
